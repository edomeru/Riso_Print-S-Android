<?xml version="1.0" encoding="UTF-8"?>
<printsettings>
    <group
        name="basic"
        text="ids_lbl_basic" >
        <setting
            name="colorMode"
            dbkey="pst_color_mode"
            text="ids_lbl_colormode"
