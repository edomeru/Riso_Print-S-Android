<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/menuContainer"
    android:layout_width="match_parent"
    android:layout_height="@dimen/home_menu_height"
    android:gravity="center"
    android:background="@drawable/selector_printsettings_print"
    android:paddingRight="@dimen/printsettings_icon_setting_padding"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/menuTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:maxLines="1"
        android:text="@string/ids_lbl_print"
        android:textAllCaps="true"
        android:textColor="@color/theme_dark_1" />

</LinearLayout>
